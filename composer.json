{
    "name": "stubbles/webapp-session",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Session classes for Stubbles based web applications.",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mikey179/vfsStream": "~1.3",
        "stubbles/core": "~5.3"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\webapp\\session\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
